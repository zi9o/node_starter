{
  "name": "api",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "engines": {
    "node": "^10.24"
  },
  "scripts": {
    "build": "babel src/ -d lib/ --copy-files",
    "prepublish": "yarn run build",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "node_modules/.bin/eslint --fix . --ext .js",
    "watch": "node_modules/.bin/gulp",
    "migrate": "npm run migration",
    "migration": "db-migrate -e ${NODE_ENV:-development} --migrations-dir=./src/database/migrations up",
    "migration:create": "db-migrate -e ${NODE_ENV:-development} --migrations-dir=./src/database/migrations create",
    "prepare": "npm run build --scripts-prepend-node-path",
    "prestart": "npm run build --scripts-prepend-node-path",
    "pre-push": "yarn lint && yarn flow",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/register": "^7.9.0",
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "debug": "^4.1.1",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "~4.16.1",
    "flow-bin": "^0.145.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.28",
    "morgan": "~1.9.1",
    "node-mailjet": "^3.3.1",
    "pg": "^7.14.0",
    "request": "^2.88.0",
    "slonik": "^22.4.8",
    "slonik-interceptor-query-logging": "^1.3.8",
    "slonik-interceptor-query-normalisation": "^1.1.10",
    "supports-color": "^7.1.0",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/db-migrate-pg": "^0.0.7",
    "@types/express": "~4.16.1",
    "@types/faker": "^5.1.2",
    "@types/gulp": "3",
    "@types/gulp-babel": "^6.1.29",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/moment-timezone": "^0.5.30",
    "@types/pg": "^7.14.3",
    "@types/slonik": "^22.1.2",
    "@types/supertest": "^2.0.10",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^26.5.2",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-sql": "^1.1.1",
    "eslint-scope": "^5.1.1",
    "gulp": "3",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "jest": "^26.5.2",
    "nodemon": "^2.0.3",
    "superagent": "^6.1.0",
    "supertest": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "<rootDir>/preprocessor.js"
    }
  }
}
